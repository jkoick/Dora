---
import Base from '@/layouts/Base.astro';
import AboutCallToAction from '@/partials/AboutCallToAction.astro';
import CallToAction from '@/partials/CallToAction.astro';
import Faq from '@/partials/Faq.astro';
import PageHeader from '@/partials/PageHeader.astro';
import PricingSection from '@/partials/Pricing.astro';
import PricingCompare from '@/partials/PricingCompare.astro';
import type { CollectionEntry } from 'astro:content';
import { getEntry } from 'astro:content';

const pricing = (await getEntry(
  'pricing',
  '-index'
)) as CollectionEntry<'pricing'>;
const { title } = pricing.data;
---

<Base {...pricing.data}>
  <PageHeader title="Serivces" />

  <PricingSection />

  <hr class="divider" />

  <!-- <PricingCompare /> -->

  <hr class="divider" />

  <!-- <Faq /> -->

  <hr class="divider" />

  <AboutCallToAction />
</Base>
