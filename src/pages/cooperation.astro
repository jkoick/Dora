---
import Base from '@/layouts/Base.astro';
import AboutBanner from '@/partials/AboutBanner.astro';
import AboutCallToAction from '@/partials/AboutCallToAction.astro';
import ClientFeedback from '@/partials/ClientFeedback.astro';
import Gallery from '@/partials/Gallery.astro';
import Team from '@/partials/Team.astro';
import TeamCopy from '@/partials/TeamCopy.astro';
import Testimonial from '@/partials/Testimonial.astro';
import WhyJoinUs from '@/partials/WhyJoinUs.astro';
import type { CollectionEntry } from 'astro:content';
import { getEntry } from 'astro:content';

const cooperation = (await getEntry(
  'cooperation',
  '-index'
)) as CollectionEntry<'cooperation'>;
---

<Base {...cooperation.data}>
  <AboutBanner />

  <hr class="divider mt-11 hidden md:block" />

  <WhyJoinUs />

  <hr class="divider" />

  <!-- <ClientFeedback /> -->

  <hr class="divider" />

  <!-- <Gallery gallery={cooperation.data.gallery} /> -->

  <Team />
  <hr class="divider" />
  <Testimonial />

  <hr class="divider" />

  <TeamCopy />

  <!-- <AboutCallToAction /> -->
</Base>
